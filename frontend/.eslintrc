{
  "extends": [
    "airbnb"
  ],
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "mocha": true
  },
  "rules": {
    "no-underscore-dangle": 0,
    "import/no-extraneous-dependencies": "off",
    "import/no-unresolved" : "off",
    "import/extensions": "off",
    "react/prop-types": "off"
  },
  "plugins": [
    "react"
  ],
  "indent": [
    "error",
    2
  ],
  "quotes": [
    "error",
    "single"
  ],
  "semi": [
    "error",
    "always"
  ],
  "comma-dangle": "always-multiline",
}
